import axios from 'axios'
//获取活动列表
export const getActivityList = function (params) {
	return axios.post('/front/activity/getActivityList', params)
}
//获取用户信息
export const getUserInfo = function (params) {
	return axios.get('/front/IntegraRecord/userIntegral', {params:params})
}
//获取签到信息
export const signIn = function (params) {
	return axios.get('/front/IntegraRecord/signIn', {params:params})
}
//获取token
export const gettoken = function (params) {
	return axios.post('/jisApi/gettoken', params)
}
